sitepackage {
    assets {
        logoPrimary = EXT:ie_site_template/Resources/Public/Images/logo-ie-white.png
        logoSecondary = EXT:ie_site_template/Resources/Public/Images/logo-ie-black.png
        heroBackground = EXT:ie_site_template/Resources/Public/Images/hero-image.jpg
        css = EXT:ie_site_template/Resources/Public/Styles/site.css
        js = EXT:ie_site_template/Resources/Public/JavaScript/site.js
    }

    layouts {
        containerWidth = 1200px
        gutter = 1.5rem
    }

    navigation {
        mainLevels = 2
        utilityLevels = 1
    }
}
